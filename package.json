{
  "name": "billiard-university-scoring",
  "version": "1.0.0",
  "description": "Billiard University Exam Scoring System",
  "main": "src/main.ts",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "vue-tsc && vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "test": "vitest",
    "lint": "eslint --ext .ts,.vue src",
    "lint:fix": "eslint --ext .ts,.vue src --fix",
    "format": "prettier --write \"**/*.{ts,vue,json,md}\"",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "pinia": "^3.0.4",
    "vue": "^3.5.28",
    "vue-router": "^5.0.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^25.3.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^10.8.0",
    "jsdom": "^28.1.0",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  },
  "keywords": [
    "billiard",
    "pool",
    "scoring",
    "exam",
    "tracking",
    "vue3",
    "typescript",
    "pinia",
    "vite",
    "sports",
    "education",
    "billiard-university",
    "8-ball",
    "9-ball",
    "skills-assessment"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acheraga/billiard-university-app.git"
  },
  "homepage": "https://billiarduniversity.org/testing/exams/",
  "bugs": {
    "url": "https://github.com/acheraga/billiard-university-app/issues"
  },
  "author": "BilliardUniversity.org",
  "license": "MIT"
}
